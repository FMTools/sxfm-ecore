<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="sxfm.presentation.SxfmModelWizardID"
            name="%_UI_SxfmModelWizard_label"
            class="sxfm.presentation.SxfmModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SxfmModelFile.gif">
         <description>%_UI_SxfmModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="sxfm.presentation.SxfmEditorID"
            name="%_UI_SxfmEditor_label"
            icon="icons/full/obj16/SxfmModelFile.gif"
            extensions="sxfm"
            class="sxfm.presentation.SxfmEditor"
            contributorClass="sxfm.presentation.SxfmActionBarContributor">
      </editor>
   </extension>

</plugin>
